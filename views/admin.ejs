<%include partials/header %>
<div class="container">
	<div class="row">
	<h1 style="text-align: center;">New music information</h1>
	<div style="width: 30%; margin: 30px auto;">
	<form class="addmusic" action="/addMusic" method="POST">
		<div class="form-group">
			<input class="form-control" type="text" name="musicname" placeholder="Music name">
		</div>
		<div class="form-group">
			<input class="form-control" type="text" name="price" placeholder="Price">
		</div>
		<div class="form-group">
			<input class="form-control" type="text" name="cover" placeholder="Cover image url">
		</div>
		<div class="form-group">
			<input class="form-control" type="text" name="style" placeholder="Style">
		</div>
		<div class="form-group">
			<input class="form-control" type="text" name="artist" placeholder="Artist">
		</div>
		<div class="form-group">
			<input class="form-control" type="text" name="review" placeholder="Editor's Review">
		</div>
		<div class="form-group">
			<input class="form-control" type="text" name="inventory" placeholder="Inventory">
		</div>
		<div class="form-group">
			<button id="addMusic" class="btn btn-lg btn-primary btn-block">Add music</button>
		</div>
		<a href="/musicstore">Go back</a>			
	</form>
	</div>		
	</div>	
</div>
<%include partials/footer%>